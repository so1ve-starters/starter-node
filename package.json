{
  "name": "pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@7.31.0",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "",
  "keywords": [],
  "type": "module",
  "homepage": "https://github.com/so1ve/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/pkg-name.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/pkg-name/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "dprint fmt",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@babel/types": "^7.21.3",
    "@so1ve/eslint-config": "^0.52.0",
    "@types/node": "^18.15.10",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "pnpm": "^7.30.3",
    "rimraf": "^4.4.1",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "unbuild": "^1.1.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  }
}
