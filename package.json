    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.17.2",
    "@babel/types": "^7.18.9",
    "@so1ve/eslint-config": "^0.26.0",
    "@types/node": "^18.6.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.20.0",
    "pnpm": "^7.7.0",
    "rimraf": "^3.0.2",
    "tsx": "^3.8.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vite": "^3.0.4",
    "vitest": "^0.19.1"
  }
