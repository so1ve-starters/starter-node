{
  "name": "pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@7.32.0",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "",
  "keywords": [],
  "type": "module",
  "homepage": "https://github.com/so1ve/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/pkg-name.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/pkg-name/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "dprint fmt",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@babel/types": "^7.21.4",
    "@so1ve/eslint-config": "^0.67.0",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "pnpm": "^7.32.0",
    "rimraf": "^5.0.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  }
}
